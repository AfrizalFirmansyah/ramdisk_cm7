#!/sbin/sh

if /sbin/busybox test -f /system/xbin/su
then
/sbin/busybox echo "su binary already installed!" 
else
/sbin/busybox echo "su binary not installed, installing..."
/sbin/busybox cp /sbin/su-bin /system/xbin/su
/sbin/busybox chmod 4755 /system/xbin/su
fi

if /sbin/busybox test -f /system/xbin/busybox
then
/sbin/busybox echo "busybox binary already installed!" 
else
/sbin/busybox echo "busybox binary not installed, installing..."
/sbin/busybox cp /sbin/busybox /system/xbin/busybox
/sbin/busybox chmod 4755 /system/xbin/busybox
fi
